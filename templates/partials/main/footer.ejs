<!-- dynamic css linking-->

<script>
	let head1 = document.getElementsByTagName("HEAD")[0];

	let link1 = document.createElement("link");
	link1.rel = 'stylesheet';
	link1.type = 'text/css';
	link1.href = "css/nav.css";
	head1.appendChild(link1);

	//linking responsive css

	let link3 = document.createElement("link");
	link3.rel = 'stylesheet';
	link3.type = 'text/css';
	link3.href = " css/navResponsive.css";
	head1.appendChild(link3);

</script>




