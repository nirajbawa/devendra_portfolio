    <script>
        let addcss = (sheetName) =>{
            let head2 = document.getElementsByTagName("HEAD")[0];
            let link2 = document.createElement("link");
            link2.rel = 'stylesheet';
            link2.type = 'text/css';
            // link2.href = 'css/' + homeFilename + '.css';
            link2.href = sheetName;
    
            head2.appendChild(link2);
        }

        addcss("/css/adminSettings.css");

        document.getElementsByClassName("nav-link")[2].classList.add("active");
        
    </script>

