			crossorigin="anonymous" referrerpolicy="no-referrer"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"
			integrity="sha512-bPs7Ae6pVvhOSiIcyUClR7/q2OAsRiovw4vAkX+zJbw3ShAeeqezq50RIIcIURq7Oa20rW2n2q+fyXBNcU9lrw=="
			crossorigin="anonymous" referrerpolicy="no-referrer"></script>


		<!-- video background -->
		<div class="video-cover">
			<div class="video-contiainer" id="cont">
				<video allowfullscreen autoplay muted loop playsinline id="myVideo">
					<source src="videos/defaultVideo.mp4" type="video/mp4">
				</video>
			</div>
		</div>

		<!-- video orverlay -->
		<div class="video-overlay"></div>


		<!-- fullpage scrolling module -->

		<div id="fullpage">

			<script>
				var text = "<%-gOSdata[2].url2%>";
				var keywords =  "<% -gOSdata[0].url4 %>";
				var videoTime = "<% -gOSdata[0].message %>";
			</script>

			<!-- home section -->
			<div class="section" class="section1" id="home">

				<div class="homeText">
					<h4>hello! my name is,</h4>
					<h1 id="headingText"><span id="mainTextspan"></span> <span class="cursor">|</span></h1>
					<h2><%-gOSdata[2].message%></h2>
					<a href="<%-gOSdata[2].url1%>" target="_blank" id="downloadButton" draggable="false"><button
							style="display: none;"><%-gOSdata[2].url4%></button></a>
				</div>

			</div>

			<!-- about section -->




			<div class="section" id="about">

				<div id="aboutmain">


					<%gAdata.forEach((values)=>{%>

						<% if(values.visibility==true){%>

							<%-values.htmlcontent%>

								<%}%>

									<%})%>




				</div>


			</div>

			<!-- interpersonal Skills section-->

			<div class="section" id="interpersonalSkills">

				<div class="intmain">
					<h1 id="ipstitle">interpersonal Skills</h1>

					<div class="owl-carousel owl-theme" style="color:white;">

						<%gISdata.forEach((values)=>{%>

							<% if(values.visibility==true){%>

								<div class="item">
									<div class="interpersonalSkillCard">
										<h3><img src="http://res.cloudinary.com/dcb6xqdug/image/upload/v1712983860/<%=values.imageurl%>"
												alt="icon1"></h3>
										<p>
											<%-values.message%>
										</p>
									</div>
								</div>

								<%}%>

									<%})%>


					</div>


				</div>
			</div>

			<!-- Technical skills -->


			<div class="section" id="professionalSkills">

				<div class="intmain">
					<h1 id="ipstitle">Technical skills</h1>



					<div class="owl-carousel owl-theme" style="color:white;">

						<%gTSdata.forEach((values)=>{%>

							<% if(values.visibility==true){%>

								<div class="item">
									<div class="interpersonalSkillCard">
										<h3><img src="http://res.cloudinary.com/dcb6xqdug/image/upload/v1712983860/<%=values.imageurl%>"
												alt="icon1"></h3>
										<p>
											<%-values.message%>
										</p>
										<p class="stars">
											<%for(let i=1; i<=values.stars; i++){%>
												<img src="images/icons/star.png" alt="">
												<%}%>
													<br>
										</p>
									</div>
								</div>

								<%}%>

									<%})%>


					</div>


				</div>


			</div>



			<!-- projects -->

			<div class="section" id="projects">

				<div class="intmain">
					<h1 id="ipstitle">Projects</h1>

					<div class="owl-carousel owl-theme" style="color:white;">

						<%gPdata.forEach((values)=>{%>

							<% if(values.visibility==true){%>

								<div class="item">
									<div class="interpersonalSkillCard">
										<h3><img src="http://res.cloudinary.com/dcb6xqdug/image/upload/v1712983860/<%=values.imageurl%>"
												alt="icon1"></h3>
										<p>
											<%-values.message%>
										</p>
										<a href="<%=values.projecturl%>" target="_blank" class="viewbnt">View
											Project</a>
									</div>
								</div>

								<%}%>

									<%})%>





					</div>


				</div>


			</div>


			<!-- Experience -->

			<div class="section" id="certificates">

				<div class="expmain">
					<div id="expl">
						<h1 id="expmainTitle">Experience</h1>
						<div>

							<%-gOSdata[0].url2%>


						</div>
					</div>

					<div id="expr">
						<div id="carouselExampleIndicators" class="carousel slide" data-bs-ride="carousel">
							<div class="carousel-inner">



								<%for(let i=0; i<edata.length; i++){%>

									<%if(edata[i].visibility==true){%>
										<%if(i==0){%>
											<div class="carousel-item active">

												<div class="expcacard">
													<div>
														<a href="<%=edata[i].cpurl%>" target="_blank" draggable="false"
															class="expa"><img
																src="http://res.cloudinary.com/dcb6xqdug/image/upload/v1712983860/<%=edata[i].imageurl%>"
																alt="" draggable="false">
															<%=edata[i].title%>
														</a>
														<p>
															<%=edata[i].timeperiod%>
														</p>
														<p class="expara">
															<%-edata[i].message%>
														</p>
														<a href="<%=edata[i].certificate%>" target="_blank"
															class="ctbtn">view certificate</a>
													</div>
												</div>

											</div>
											<%}else{%>
												<div class="carousel-item">

													<div class="expcacard">
														<div>
															<a href="<%=edata[i].cpurl%>" target="_blank"
																draggable="false" class="expa"><img
																	src="http://res.cloudinary.com/dcb6xqdug/image/upload/v1712983860/<%=edata[i].imageurl%>"
																	alt="" draggable="false">
																<%=edata[i].title%>
															</a>
															<p>
																<%=edata[i].timeperiod%>
															</p>
															<p class="expara">
																<%-edata[i].message%>
															</p>
															<a href="<%=edata[i].certificate%>" target="_blank"
																class="ctbtn">view certificate</a>
														</div>
													</div>

												</div>
												<%}%>
													<%}%>
														<%}%>





							</div>
							<button class="carousel-control-prev" type="button"
								data-bs-target="#carouselExampleIndicators" data-bs-slide="prev">
								<span class="carousel-control-prev-icon" aria-hidden="true">
									<script
										src="https://unpkg.com/@lottiefiles/lottie-player@latest/dist/lottie-player.js"></script>
									<lottie-player src="https://assets8.lottiefiles.com/packages/lf20_0nj7MXnOG7.json"
										background="transparent" speed="1" loop autoplay></lottie-player>
								</span>
								<span class="visually-hidden">Previous</span>
							</button>
							<button class="carousel-control-next" type="button"
								data-bs-target="#carouselExampleIndicators" data-bs-slide="next">
								<span class="carousel-control-next-icon" aria-hidden="true">
									<script
										src="https://unpkg.com/@lottiefiles/lottie-player@latest/dist/lottie-player.js"></script>
									<lottie-player src="https://assets8.lottiefiles.com/packages/lf20_0nj7MXnOG7.json"
										background="transparent" speed="1" loop autoplay></lottie-player>
								</span>
								<span class="visually-hidden">Next</span>
							</button>
						</div>



					</div>


				</div>


			</div>



			<!-- contact -->


			<div class="section" id="contact">


				<div id="contmain">
					<div class="conleft">


						<h1>Contact Me !</h1>

						<form id="contactform" enctype="application/x-www-form-urlencoded">

							<div class="form-floating">
								<input type="email" class="form-control" id="floatingInput"
									placeholder="name@example.com" name="email" required>
								<label for="floatingInput">Email address</label>
							</div>

							<div class="confteldiv">
								<div>
									<select class="form-select form-select-lg form-floating"
										aria-label=".form-select-lg example" name="countrycode" style="height:100%;"
										id="countrycode">
										<option selected>+</option>
									</select>
								</div>


								<div class="form-floating">
									<input type="tel" class="form-control" id="floatingInput" name="mono"
										placeholder="name@example.com" autocomplete="off">
									<label for="floatingInput">Phone No</label>
								</div>
							</div>


							<div class="form-floating">
								<textarea class="form-control" placeholder="Leave a comment here" name="message"
									id="floatingTextarea2"></textarea>
								<label for="floatingTextarea2">Message</label>
							</div>

							<button type="submit" id="conformsubbtn">submit</button>

						</form>


						<script>


						</script>

					</div>

					<div class="conright">
						<h1>Contact Me !</h1>
						<div>
							<%-gOSdata[0].url3%>

						</div>

					</div>
				</div>


			</div>



		</div>

		<footer>

			<div id="socialicons" style="display: none;">
				<a href="<%=gOSdata[1].url3%>" target="__blank__"><i class="fa fa-github" aria-hidden="true"></i></a>
				<a href="<%=gOSdata[1].url2%>" target="__blank__"><i class="fa fa-linkedin" aria-hidden="true"></i></a>
			</div>
		</footer>



